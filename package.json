{
    "name": "indexed-memory-storage",
    "version": "1.1.0",
    "keywords": [
        "index",
        "indexed",
        "memory",
        "storage",
        "in-memory"
    ],
    "description": "Storage engine with fast getters for indexed data.",
    "main": "dist/storage.cjs.js",
    "module": "dist/storage.es.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "prepublish": "npm run build",
        "test": "yarn lint && jest --coverage --no-cache",
        "test:watch": "jest --watch",
        "lint": "eslint ./src/**/*.{ts,tsx}",
        "lint:fix": "lint --fix",
        "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md,graphl}\" --ignore-path .gitignore",
        "prebuild": "rimraf dist",
        "build": "tsc --module commonjs --noEmit && rollup -c rollup.config.ts --configPlugin typescript",
        "postbuild": "rimraf dist/dts",
        "start": "rollup -c rollup.config.ts -w --configPlugin typescript",
        "prepare": "husky"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DJWassink/indexed-memory-storage.git"
    },
    "author": "Dirk Jan Wassink",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DJWassink/indexed-memory-storage/issues"
    },
    "homepage": "https://github.com/DJWassink/indexed-memory-storage#readme",
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,css,scss,md,graphl,js,jsx}": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "rxjs": ">=7"
    },
    "peerDependencies": {
        "rxjs": ">=7"
    },
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.19",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "rollup": "^4.11.0",
        "rollup-plugin-dts": "^6.1.0",
        "ts-jest": "^29.1.2",
        "typescript": "^5.3.3"
    }
}
